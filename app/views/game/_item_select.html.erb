<div class="body-sec">
  <%= form_with do |form| %>
    <% @game.get_bag_items.each do |item| %>
        <div class="form-check">
            <%= form.check_box :item, class: "form-check-input"%>
            <%# <input class="form-check-input" type="checkbox" value="" id="defaultCheck1"> %>
            <%# <%= form.label :name %>
            <label class="form-check-label" for="defaultCheck1">
                <%# TODO: renderer for items %>
                <%= item.to_s%>
            </label>
        </div>
    <% end %>
    <div class="actions">
      <%= form.submit "Load Cup", class: "btn btn-light" %>
    </div>
  <% end %>
</div>
